<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.exdiary.web_backend.mapper.UserMapper">
    <!-- 유저 회원가입 하기 -->
    <insert id="insetUser" parameterType="com.exdiary.web_backend.dto.UserDTO">
        INSERT INTO user (email,password,joindate,phone,profile_img,nickname,gender,token)
        VALUES( #{email},#{password}, #{joindate}, #{phone}, #{profile_img}, #{nickname}, #{gender}, #{token})
    </insert>
</mapper>